<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.my.smokingsetting.SmokingSettingFinishFragment">


    <TextView
        android:id="@+id/tv_onboarding_perpack1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="450dp"
        android:fontFamily="@font/pretendard_bold"
        android:text="재설정을 모두 완료하였어요!"
        android:textColor="@color/primary_blue"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_reseting_onboarding_next"
        android:layout_width="330dp"
        android:layout_height="54dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/bg_onboarding_button"
        android:fontFamily="@font/pretendard_semi_bold"
        android:text="마이페이지로 돌아가기"
        android:enabled="true"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>